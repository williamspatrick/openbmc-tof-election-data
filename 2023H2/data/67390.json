                {
                    "file": "src/PSUSensorMain.cpp",
                    "line": 807,
                    "reviewer": {
                        "name": "Cosmo Chou",
                        "email": "cosmo.chou@quantatw.com",
                        "username": "cchoux"
                    },
                    "message": "nameIndex is only used in non-customizedName cases.\nIn customizedName cases, the customized Names (from entity-manager) are used instead of psuProperty->labelTypeName."
                },
                {
                    "file": "src/PSUSensorMain.cpp",
