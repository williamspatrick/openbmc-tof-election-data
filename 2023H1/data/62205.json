                    "reviewer": {
                        "name": "William A. Kennington III",
                        "email": "wak@google.com",
                        "username": "wak-google"
                    },
                    "message": "Make this filename `-ether-bridge.network` to guarantee it takes precedence over the phosphor-networkd `00-` rules."
                },
                {
                    "file": "meta-google/recipes-google/networking/gbmc-bridge.bb",
                    "line": 84,
