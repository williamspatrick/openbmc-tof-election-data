                    "reviewer": {
                        "name": "PeterYinGit",
                        "email": "peter.yin@quantatw.com",
                        "username": "PeterYinGit"
                    },
                    "message": "Yes,\n\n\u2502 \u251c\u2500 /xyz/openbmc_project/inventory/system/board/Harma_MB\n\u2502 \u251c\u2500 /xyz/openbmc_project/inventory/system/board/Harma_SCM\n\u2502 \u2514\u2500 /xyz/openbmc_project/inventory/system/board/Minerva_PDBC\n\u2514\u2500 /xyz/openbmc_project/inventory/system/chassis\n\u2514\u2500 /xyz/openbmc_project/inventory/system/chassis/Harma_Chassis"
                },
