    "patchSets": [
        {
            "number": 1,
            "revision": "a932a0a46c1d7359bd2c4f9c3a6275faec19b4d1",
            "parents": [
                "2292498a06532ca2f5ab3f74c591c0d24ff2eb8f"
            ],
            "ref": "refs/changes/07/50207/1",
            "uploader": {
                "name": "Ed Tanous",
