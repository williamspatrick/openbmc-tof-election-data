                {
                    "file": "meta-google/recipes-google/networking/gbmc-mac-config.bb",
                    "line": 24,
                    "reviewer": {
                        "name": "William A. Kennington III",
                        "email": "wak@google.com",
                        "username": "wak-google"
                    },
                    "message": "I've been using GBMC_* for all of our configuration variables to ensure we don't end up colliding with variables used by other projects. Especially since that variable enables the use of this daemon, we don't want others falling into this accidentally."
                }
